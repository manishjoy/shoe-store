<?xml version="1.0"?>
<config>
    <tabs>
        <newtab translate="label" module="customer">
            <label>Excellence Test</label>
            <sort_order>1</sort_order>
        </newtab>
    </tabs>
    <sections>
        <testsection translate="label" module="fav">
            <class>separator-top</class>
            <label>User Details</label>
            <tab>newtab</tab>
            <sort_order>130</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <test translate="label">
                    <label>Enter User Details</label>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>0</show_in_website>
                    <show_in_store>0</show_in_store>
                    <fields>
                        <name translate="label comment">
                            <label>Name</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Enter your name.</comment>
                        </name>
                        <patient translate="label">
                            <label>Country</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_country</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </patient>
                    </fields>
                    <fields>
                        <slider translate="label">
                            <label>Skills</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>test/system_config_source_interest</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                       </slider>
                    </fields>
                     
                </test>
                </groups>
            </testsection>
    </sections>
</config>